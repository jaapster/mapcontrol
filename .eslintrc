{
  "parser": "babel-eslint",
  "plugins": ["babel"],

  "env": {
    "browser": true,
    "node": true
  },

  "rules": {
    "comma-dangle": ["error", "never"],
    "no-underscore-dangle": 0,
    "no-param-reassign": 0,
    "no-tabs": 0,
    "max-len": 0,
    "no-console": 0,
    "indent": ["error", "tab", { "SwitchCase": 1 }],
    "jsx-a11y/show-errors": 0,
    "import/prefer-default-export": 0,
    "import/first": 0,
    "no-bitwise": 0,
    "no-plusplus": 0,
    "no-nested-ternary": 0
  }
}

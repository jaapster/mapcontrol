{
  "name": "mapcontrol",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "app": "node ./src/js/server/index.js",
    "build": "webpack --progress --colors && cp src/html/*.* dist && cp src/css/*.* dist",
    "watch": "webpack --progress --colors --watch",
    "tileserver": "cd ~/Documents/Kitematic/tileserver-gl/data && docker run --rm -it -v $(pwd):/data -p 1981:80 klokantech/tileserver-gl",
	"start": "npm run build && concurrently --kill-others \"npm run app\" \"npm run watch\""
  },
  "dependencies": {
    "axios": "^0.16.2",
    "express": "^4.15.3",
    "pbf": "^3.1.0",
    "simplify": "^1.0.0",
    "tesspathy": "^0.1.1"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0-beta.49",
    "@babel/core": "7.0.0-beta.49",
    "@babel/polyfill": "7.0.0-beta.49",
    "@babel/preset-env": "7.0.0-beta.49",
    "@babel/preset-flow": "7.0.0-beta.49",
    "@babel/preset-stage-0": "7.0.0-beta.49",
    "babel-eslint": "^8.2.1",
    "babel-loader": "^8.0.0-beta",
    "eslint": "^4.19.1",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.12.0",
    "flow-bin": "^0.73.0",
    "path": "^0.12.7",
    "source-map-support": "^0.5.6",
    "webpack": "^4.10.2",
    "webpack-cli": "^3.0.1",
    "webpack-glsl-loader": "^1.0.1",
    "worker-loader": "^2.0.0"
  }
}

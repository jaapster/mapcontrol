{
  "name": "mapcontrol",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "app": "node ./src/js/server/index.js",
    "build": "webpack --progress --colors && cp src/html/*.* dist && cp src/css/*.* dist",
    "watch": "webpack --progress --colors --watch",
    "tileserver": "cd ~/Documents/Kitematic/tileserver-gl/data && docker run --rm -it -v $(pwd):/data -p 1981:80 klokantech/tileserver-gl",
	"start": "npm run build && concurrently --kill-others \"npm run app\" \"npm run watch\""
  },
  "dependencies": {
    "axios": "0.19.0-beta.1",
    "express": "4.16.3",
    "pbf": "3.1.0",
    "tesspathy": "0.1.1"
  },
  "devDependencies": {
    "@babel/cli": "7.0.0",
    "@babel/core": "7.0.0",
    "@babel/plugin-proposal-class-properties": "7.0.0",
    "@babel/plugin-proposal-decorators": "7.0.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.0.0",
    "@babel/plugin-proposal-optional-chaining": "7.0.0",
    "@babel/plugin-proposal-pipeline-operator": "7.0.0",
    "@babel/plugin-syntax-dynamic-import": "7.0.0",
    "@babel/plugin-transform-runtime": "7.0.0",
    "@babel/polyfill": "7.0.0",
    "@babel/preset-env": "7.0.0",
    "@babel/preset-flow": "7.0.0",
    "@babel/preset-react": "7.0.0",
    "@babel/register": "7.0.0",
    "babel-eslint": "8.2.1",
    "babel-loader": "8.0.2",
    "eslint": "4.19.1",
    "eslint-plugin-babel": "5.1.0",
    "eslint-plugin-import": "2.12.0",
    "flow-bin": "0.73.0",
    "path": "0.12.7",
    "source-map-support": "0.5.6",
    "webpack": "4.12.0",
    "webpack-cli": "3.0.3",
    "webpack-glsl-loader": "1.0.1",
    "worker-loader": "2.0.0"
  }
}
